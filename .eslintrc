{
  "env": {
      "node": true,
      "es6": true
  },
  "rules": {
    "no-var": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "guard-for-in": 0,
    "func-names": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "curly": [
        2,
        "multi-line"
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "indent": [2, 4, {"SwitchCase": 1}],
    "consistent-return": 0,
    "no-plusplus": [2, {"allowForLoopAfterthoughts": true}],
    "no-unused-vars": ["error", {"args": "none"}],
    "prefer-const": [2, {
        "destructuring": "all"
    }]
  }
}
